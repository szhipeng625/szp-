// 大胆猜测，从2我们猜出最后一位为1必胜，反之必败
#include<bits/stdc++.h>

using namespace std;

using i64 = long long;

void solve() {
    i64 x;
    cin >> x;
    for(int i = 2; i <= x; i ++){
        if(x % i != 0 || i == x){
            cout << i << endl;
            return ;
        }
    }

}

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin >> n;

    for (int i = 0; i < n; i++) {
        solve();
    }
    return 0;
}
